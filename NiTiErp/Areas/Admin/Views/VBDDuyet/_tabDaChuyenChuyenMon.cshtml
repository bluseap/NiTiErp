                </thead>
                <tbody id="tblContentDaChuyenChuyenMon"></tbody>
            </table>
            <div class="row">
                <div class="col-sm-5">
                    <div class="dataTables_info" id="datatable-checkbox_infoDaChuyenChuyenMon" role="status" aria-live="polite">
                        <select id="ddl-show-pageDaChuyenChuyenMon">
                            <option value="10" selected="selected">10</option>
                            <option value="20">20</option>
                            <option value="30">30</option>
                            <option value="50">50</option>
                        </select>
                        <span class="item-per-page">

<script id="table-DaChuyenChuyenMon" type="x-tmpl-mustache">
    <tr>
        <th scope="row">
            <a class="btn btn-lg btnDCCCMChuyen" data-id="{{Id}}" data-toggle="tooltip" data-placement="top" title="Đã chuyển chuyên môn">
                <i class="fa fa-pencil"></i>
                Đã chuyển
            </a>
        </th>
        <td>{{TenSoVanBanDen}}</td>
        <td>
            <a class="btn btn-link btnDCCCMPatchFileTrichYeu" data-id="{{Id}}" data-toggle="tooltip">
                {{TrichYeuCuaVanBan}}
            </a>
        </td>
        <td>
            <a class="btn btn-link btnDCCCMPatchFileKyHieu" data-id="{{Id}}" data-toggle="tooltip">
                {{SoKyHieuDen}}
            </a>
        </td>
        <td>{{TenCoQuanBanHanh}}</td>
        <td>{{NgayBanHanhCuaVanBan}}</td>
        <td>{{NgayDenCuaVanBan}}</td>
        <td>{{{TTXuLy}}}</td>

